<p id="notice"><%= notice %></p>

<div class="panel panel-default">
  <div class="panel-heading"><h2>Lista projektow</h2></div>
  <div class="panel-body">
    
<table>
  <thead>
    <tr>
      <th>Obraz</th>
      <th>Opis</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= image_tag pin.image.url (:medium) %></td>
        <td><%= pin.description %></td>
        <td><%= link_to 'Wyswietl', pin %></td>
        <td><%= link_to 'Edycja', edit_pin_path(pin) %></td>
        <td><%= link_to 'Usun', pin, method: :delete, data: { confirm: 'Jesetes pewien?' } %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

